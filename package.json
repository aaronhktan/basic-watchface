        "resources": {
            "media": [
                {
                    "characterRegex": "[0-9:%a-zA-Z .,]",
                    "file": "fonts/LECO1976.ttf",
                    "name": "LECO_20",
                    "targetPlatforms": null,
                    "type": "font"
                }
            ]
        },
        "sdkVersion": "3",
        "targetPlatforms": [
            "aplite",
            "basalt",
            "chalk",
            "diorite",
            "emery"
        ],
        "uuid": "cb786667-e9e3-4579-8900-4b9fc50f7ea4",
        "watchapp": {
            "watchface": true
        }
    },
    "version": "1.1.0"
