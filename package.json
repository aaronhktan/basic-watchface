        "capabilities": [
            "location",
            "configurable",
            "health"
        ],
        "displayName": "Basic Watchface",
        "enableMultiJS": true,
        "messageKeys": [
            "healthEnabled",
            "weatherEnabled",
            "disconnectEnabled",
            "hourlyVibrationEnabled",
            "weatherProvider",
            "temperatureUnit",
            "owmKey",
            "wuKey",
            "dsKey",
            "temperature",
            "conditions",
            "pmEmail",
            "pmPIN"
